#include<stdio.h>
include<string.h>
int main()
{
    char str[1000];
    gets(str);
    int a,b,sum=str[0]-'0';
    for(int j=1;j<strlen(str);i++)
    {
        switch(str[j])
        {
            case'*':str[j-1]-='0';str[j-1]*=str[j+1]-'0';a=j-1;break;
            case'/':str[j-1]-='0';str[j-1]/=str[j+1]-'0';b=j-1;break;
        }
    }
    for(int i=1;i<strlen(str);i++)
    {
        switch(str[j])
        {
            case'+':if((i+1)==a)sum+=str[a];
                    else if((i+1)==b)sum+=str[b];
                    else sum+=str[i+1]-'0';break;
            case'-':if((i+1)==a)sum-=str[a];
                    else if((i+1)==b)sum-=str[b];
                    else sum-=str[i+1]-'0';break;
        }
    }
    printf("sum=%d\n",sum);
}
